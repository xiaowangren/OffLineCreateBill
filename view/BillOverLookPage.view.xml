<core:View xmlns:core="sap.ui.core" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m" controllerName="com.zhenergy.bill.view.BillOverLookPage" 
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page
	    title="两票离线打印平台"
		showHeader="true"
		enableScrolling="false" >
		<!--tiles="{/TileCollection}"-->
						<!--info="最近更新:12.15 16:00"-->
		<TileContainer
			id="container"
			tileDelete="handleTileDelete"
			width="80%"
			>
			<StandardTile
			    id="tileSync"
				icon="sap-icon://inbox"
				title="同步主数据"
				info="最近更新:{/lastSyncLog}"
				infoState="Success"
				press="onSyncMasterData"
				/>
			<StandardTile
			    id="tileCreateOT"
				icon="sap-icon://order-status"
				title="创建操作票"
				infoState="Success"  
				press="onNavigate">
				<customData>
					<core:CustomData key="targetPage" value="BillInitializationPage">
					</core:CustomData>
				</customData>
			</StandardTile>
			<StandardTile
			    id="tileQueryOT"
				icon="sap-icon://order-status"
				title="查询操作票"
				infoState="Success"  
				press="onQueryCaoZuoPiao">
				<customData>
					<core:CustomData key="targetPage" value="BillInitializationPage">
					</core:CustomData>
				</customData>
			</StandardTile>
			<StandardTile
			    id="tileModifyOT"
				icon="sap-icon://order-status"
				title="修改操作票"
				infoState="Error"
				press="onUpdateCaoZuoPiao">
				<customData>
					<core:CustomData key="targetPage" value="BillInitializationPage">
					</core:CustomData>
				</customData>
			</StandardTile>
			<!--info="最近上传:12.15 16:00"-->
			<StandardTile
			    id="tileUpload"
				icon="sap-icon://hint"
				type="Monitor"
				title="上传离线数据"
				
				info="最近上传:{/lastUpload}"
				infoState="Success"
				press="onUploadToEcc"
				/>
			<StandardTile
			    id="tileCreateWT"
				icon="sap-icon://factory"
				title="创建工作票"
				infoState="Success"
				press="onCreateGongZuoPiao">
				<customData>
					<core:CustomData key="targetPage" value="BillInitializationPage">
					</core:CustomData>
				</customData>
			</StandardTile>
			<StandardTile
			    id="tileQueryWT"
				icon="sap-icon://factory"
				title="查询工作票"
				infoState="Error"
				press="onQueryGongZuoPiao">
				<customData>
					<core:CustomData key="targetPage" value="BillInitializationPage">
					</core:CustomData>
				</customData>
			</StandardTile>
			<StandardTile
			    id="tileModifyWT"
				icon="sap-icon://factory"
				title="修改工作票"
				infoState="Error"
				press="onUpdateGongZuoPiao">
				<customData>
					<core:CustomData key="targetPage" value="BillInitializationPage">
					</core:CustomData>
				</customData>
			</StandardTile>
		</TileContainer>
		<footer>
			<Toolbar>
				<ToolbarSpacer/>
					<Button text="Print" press="handlePrintPress" />
				<ToolbarSpacer/>
			</Toolbar>
		</footer>
	</Page>
</core:View>